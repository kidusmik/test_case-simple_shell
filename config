# Configuration file

TESTDIR="."
R=$RANDOM
OUTSYSFILENAME="checker_sys_output_$R"
OUTSSFILENAME="checker_ss_output_$R"
OUTERRSYSFILENAME="checker_sys_err_output_$R"
OUTERRSSFILENAME="checker_ss_err_output_$R"
OUTLTRACEFILENAME="checker_ltrace_output_$R"
TMPDIR="/tmp"
OUTSYS="$TMPDIR/$OUTSYSFILENAME"
OUTSS="$TMPDIR/$OUTSSFILENAME"
OUTERRSYS="$TMPDIR/$OUTERRSYSFILENAME"
OUTERRSS="$TMPDIR/$OUTERRSSFILENAME"
OUTLTRACE="$TMPDIR/$OUTLTRACEFILENAME"
SHELL="./simple_shell"
SHELLSYS="/bin/sh"
SLEEP=`which sleep`
SLEEPSECONDS=1
LTRACE=`which ltrace`
ECHO=`which echo`
CAT=`which cat`
GREP=`which grep`
WC=`which wc`
RM=`which rm`
TOUCH=`which touch`
CHMOD=`which chmod`
CMP=`which cmp`
